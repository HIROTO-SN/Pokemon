ults);\r\n\r\n    // 次の次のページのURLをセット\r\n    setNextURL(newPokemonData.next);\r\n\r\n    // 前のページのURLをセット\r\n    setPrevURL(newPokemonData.previous);\r\n\r\n    setLoading(false);\r\n  };\r\n  return (\r\n    <>\r\n      <Navbar initialURL={initialURL} handlePrevPage={handlePrevPage} handleNextPage={handleNextPage} nextURL={nextURL} prevURL={prevURL}/>\r\n      <div className=\"App\">\r\n        {loading ? (\r\n          <Load />\r\n        ) : (\r\n          <>\r\n            {/* <h1>ポケモンデータを取得しました</h1> */}\r\n            <div className=\"pokemonCardContainer\">\r\n              {pokemonData.map((pokemon, i) => {\r\n                return <Card key={i} pokemon={pokemon} />;\r\n              })}\r\n            </div><br />\r\n          </>\r\n        )}\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\hirot\\Desktop\\react-3-apps\\pokemon-app\\pokedex\\frontend\\src\\utils\\pokemon.js",[],[],"C:\\Users\\hirot\\Desktop\\react-3-apps\\pokemon-app\\pokedex\\frontend\\src\\components\\Navbar\\Navbar.js",[],[],"C:\\Users\\hirot\\Desktop\\react-3-apps\\pokemon-app\\pokedex\\frontend\\src\\components\\Load\\Load.js",[],[],"C:\\Users\\hirot\\Desktop\\react-3-apps\\pokemon-app\\pokedex\\frontend\\src\\components\\Card\\Card.js",["35"],[],"import React from \"react\";\r\nimport \"./Card.css\";\r\n\r\nconst Card = ({ pokemon }) => {\r\n  return (\r\n    <div className=\"card\">\r\n      <div className=\"img\">\r\n        <img src={pokemon.sprites.front_default} />\r\n      </div>\r\n      <h3 className=\"cardName\">{pokemon.name}</h3>\r\n      <div className=\"cardTypes\">\r\n        <div>タイプ</div>\r\n        {pokemon.types.map((_type) => {\r\n          return (\r\n            <div key={_type.type.name}>\r\n              <span className=\"typeName\">{_type.type.name}</span>\r\n            </div>\r\n          );\r\n        })}\r\n      </div>\r\n\t\t\t<div className=\"cardInfo\">\r\n\t\t\t\t<div className=\"cardData\">\r\n\t\t\t\t\t<p className